    return response as {{{rpcs.returnsTypeName}}};
    {{/rpcs.rpcReturnType.returnsScalar}}
    {{^rpcs.rpcReturnType.returnsScalar}}
    return (response as List).map((e) => {{rpcs.returnsClassName}}.fromJson(e as Map<String, dynamic>)).toList();
    {{/rpcs.rpcReturnType.returnsScalar}}
