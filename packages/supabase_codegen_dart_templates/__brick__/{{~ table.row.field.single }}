  /// {{fieldName.titleCase()}}
  {{{dartType}}}{{field.question}} get {{fieldName}} => getField<{{{dartType}}}>(
    {{fieldName}}Field{{#isEnum}}, enumValues: {{{dartType}}}.values{{/isEnum}}{{#hasDefault}}, defaultValue: {{{field.defaultValue}}}{{/hasDefault}}
  ){{field.bang}};
  set {{fieldName}}({{{dartType}}}{{field.question}} value) => setField<{{{dartType}}}>({{fieldName}}Field, value);
